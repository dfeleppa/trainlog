<h1>Welcome to TRAINlog</h1>
  
<h3> Login Here </h3>

<%= form_for :user, url: { action: "create" } do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
    <%= f.submit "Login" %>
<% end %>

<p> New user?
<br>
<%= link_to "Sign Up", new_user_path  %>
</p>