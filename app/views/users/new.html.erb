<h1> User Sign Up </h1>

<%= form_for @user do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email, required:true %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

        <%= f.submit "Sign Up" %>
<% end %>
